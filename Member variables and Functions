class Animal(object):
    """Makes cute animals."""
    is_alive = True
    health = "good"
    def __init__(self, name, age):
        self.name = name
        self.age = age
    # Add your method here!
    def description(self):
        print self.name
        print self.age
hippo = Animal("Jack",12)
hippo.description
sloth = Animal("Jhon",2)
ocelot = Animal("Kater",3)
print hippo.health
print sloth.health
print ocelot.health
